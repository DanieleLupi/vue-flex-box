{
  "name": "@sidline/vue-flex-box",
  "version": "1.0.15",
  "description": "A flexbox grid system.",
  "keywords": [
    "flex",
    "box",
    "vue"
  ],
  "license": "MIT",
  "author": {
    "name": "Daniele Lupi",
    "email": "daniele.lupi@sidline.it"
  },
